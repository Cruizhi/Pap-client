<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.pap">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.administrator.activity.ChoiceLogin" />
        <activity android:name="com.example.administrator.activity.Login" />
        <activity android:name="com.example.administrator.activity.Register" />
        <activity android:name="com.example.administrator.activity.SalesGoods" />
        <activity android:name="com.example.administrator.activity.GoodsDetailView" />
        <activity android:name="com.example.administrator.activity.Text" />
        <activity android:name="com.example.administrator.activity.Indent" />
        <activity android:name="com.example.administrator.activity.Setting" />
        <activity android:name="com.example.administrator.activity.WareActivity" />
        <activity android:name="com.example.administrator.activity.FostIndent" />
        <activity android:name="com.example.administrator.activity.FostInfo" />
        <activity android:name="com.example.administrator.activity.FosterageDetail" />
        <activity android:name="com.example.administrator.activity.FinishIndent" />
    </application>

</manifest>